            FALSE = 1'd0;
            TRUE = 1'd1;
        };
        field {
            encode          = D_DATAOUT_MAP_LINE_PACKED_enum;
            sw              = rw;
            hw              = r;
            spec_access     = rw;
            reset           = 0x0;
            reset_mask      = 0x1;
