            reset_mask      = 0x1f;
            spec_sw_default = 0x0;
            sw_default_mask = 0x0;
        } BATCHES[4:0];
        BATCHES->desc="
////batch number - 1
";
    }D_BATCH_NUMBER @0x1c;
    reg {
        name = "D_LINE_STRIDE";
        field {
            sw              = rw;
            hw              = r;
            spec_access     = rw;
            reset           = 0x0;
            reset_mask      = 0xffffff;
            spec_sw_default = 0x0;
            sw_default_mask = 0x0;
        } LINE_STRIDE[23:0];
        LINE_STRIDE->desc="
////bytes
";
    }D_LINE_STRIDE @0x20;
    reg {
        name = "D_SURF_STRIDE";
        field {
            sw              = rw;
            hw              = r;
            spec_access     = rw;
            reset           = 0x0;
            reset_mask      = 0xffffff;
            spec_sw_default = 0x0;
            sw_default_mask = 0x0;
        } SURF_STRIDE[23:0];
        SURF_STRIDE->desc="
////bytes
