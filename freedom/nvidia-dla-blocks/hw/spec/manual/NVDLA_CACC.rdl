            spec_sw_default = 0x0;
            sw_default_mask = 0x0;
        } DATAOUT_CHANNEL[12:0];
        DATAOUT_CHANNEL->desc="
////Output data cube channel - 1
";
    }D_DATAOUT_SIZE_1 @0x14;
    reg {
        name = "D_DATAOUT_ADDR";
        field {
            sw              = rw;
            hw              = r;
            spec_access     = rw;
            reset           = 0x0;
            reset_mask      = 0xffffffff;
            spec_sw_default = 0x0;
            sw_default_mask = 0x0;
        } DATAOUT_ADDR[31:0];
        DATAOUT_ADDR->desc="
////Output data address
";
    }D_DATAOUT_ADDR @0x18;
    reg {
        name = "D_BATCH_NUMBER";
        field {
            sw              = rw;
            hw              = r;
            spec_access     = rw;
